&HEAD CHID='pine_N2_25_3C', TITLE='Kashiwagi et al. Combust. Flame (1987), white pine gasification, pure N2, 25 kW/m2'/

&MESH IJK=16,16,16, XB=-0.03,0.05,-0.04,0.04,-0.04,0.04/

&TIME T_END=900., WALL_INCREMENT=1/

&DUMP DT_DEVC=10., DT_PROF=10./

&VENT DB='ZMIN', SURF_ID='inflow'/
&VENT DB='ZMAX', SURF_ID='OPEN'/

&SURF ID='inflow', VEL=-0.005, COLOR='BLUE'/
&SURF ID='sample',         STRETCH_FACTOR=1, HT3D=T, MATL_ID(1,1:3) = 'PINE 1','PINE 2','PINE 3', MATL_MASS_FRACTION(1,1:3) = 0.55,0.1,0.35, MOISTURE_FRACTION(1:3)=0.05,0.05,0.05, COLOR='BROWN'/
&SURF ID='exposed sample', STRETCH_FACTOR=1, HT3D=T, MATL_ID(1,1:3) = 'PINE 1','PINE 2','PINE 3', MATL_MASS_FRACTION(1,1:3) = 0.55,0.1,0.35, MOISTURE_FRACTION(1:3)=0.05,0.05,0.05, COLOR='GRAY 10', EXTERNAL_FLUX=25./
&SURF ID='default wall', EMISSIVITY=0., HEAT_TRANSFER_COEFFICIENT=0., COLOR='INVISIBLE', FREE_SLIP=T, DEFAULT=T/

&OBST XB=0,0.038,-0.019,0.019,-0.019,0.019, SURF_ID6='exposed sample','sample','sample','sample','sample','sample'/

&SPEC ID='NITROGEN', BACKGROUND=T/
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.0/
&SPEC ID='CARBON DIOXIDE'/
&SPEC ID='FUEL VAPOR'/
&SPEC ID='WATER VAPOR'/

! simple material for setup
! &MATL ID='PINE', DENSITY=360., SPECIFIC_HEAT=1.15, CONDUCTIVITY=0.3 /

! full material model with kinetics
&CATF OTHER_FILES='../../../Utilities/Input_Libraries/MATL/pine_wood_3C_MATL.fds'/

&BNDF QUANTITY='MASS FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='BACK WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='TOTAL HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='RADIATIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='CONVECTIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='WALL THICKNESS', CELL_CENTERED=T/
&BNDF QUANTITY='SURFACE DENSITY', CELL_CENTERED=T/
&BNDF QUANTITY='SUBSTEPS', CELL_CENTERED=T/

&DEVC XB=-0.03,0.05,-0.04,0.04,-0.04,0.04, QUANTITY='MASS FLUX', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='mf', CONVERSION_FACTOR=62500, UNITS='mg/cm2 s'/
&DEVC XB=-0.03,0.05,-0.04,0.04,-0.04,0.04, QUANTITY='SURFACE DENSITY', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='MASS'/

&CTRL ID='PID WT LOSS', FUNCTION_TYPE='PID', INPUT_ID='MASS', PROPORTIONAL_GAIN=0, DIFFERENTIAL_GAIN=1, INTEGRAL_GAIN=0/
&DEVC XYZ=0,0,0, ID='wt loss mf', QUANTITY='CONTROL VALUE', CTRL_ID='PID WT LOSS', CONVERSION_FACTOR=-62500, UNITS='mg/cm2 s'/

&DEVC XYZ=-0.001,0,0, QUANTITY='WALL TEMPERATURE', IOR=-1, ID='T0mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.005, IOR=-1, ID='T5mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.010, IOR=-1, ID='T10mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.015, IOR=-1, ID='T15mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='BACK WALL TEMPERATURE', IOR=-1, ID='T38mm'/

&PROF ID='Tx' , XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', IOR=-1 /
&PROF ID='P1x', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='PINE 1', IOR=-1 /
&PROF ID='P2x', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='PINE 2', IOR=-1 /
&PROF ID='P3x', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='PINE 3', IOR=-1 /
&PROF ID='CHARx', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='CHAR', IOR=-1 /
&PROF ID='ASHx' , XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='ASH' , IOR=-1 /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/

&TAIL /
