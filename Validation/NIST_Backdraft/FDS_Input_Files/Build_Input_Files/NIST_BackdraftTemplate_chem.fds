&DEVC ID='T2', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.394/
&DEVC ID='T3', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.197 /
&DEVC ID='T4', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.375,-0.02,0.94 /
&DEVC ID='T5', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.375,-0.02,0.24 /
&DEVC ID='T6', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.86 ,-0.02,0.94 /
&DEVC ID='T7', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.86 ,-0.02,0.24 /

