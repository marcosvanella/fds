&HEAD CHID='paramCHID',TITLE='paramTITLE' /

&TIME T_END=10. /
&MISC SIMULATION_MODE='LES' /
&COMB EXTINCTION_MODEL='EXTINCTION 2' /
&CLIP MINIMUM_DENSITY=0.01/
&PRES SOLVER='ULMAT', MAX_PRESSURE_ITERATIONS=3/
&VENT MB='XMIN',SURF_ID='OPEN' /
&VENT MB='XMAX',SURF_ID='OPEN' /
&VENT MB='YMIN',SURF_ID='OPEN' /
&VENT MB='YMAX',SURF_ID='OPEN' /
&VENT MB='ZMIN',SURF_ID='OPEN' /
&VENT MB='ZMAX',SURF_ID='OPEN' /

&SPEC ID='NITROGEN', MASS_FRACTION_0=0.77, BACKGROUND=.TRUE./
&SPEC ID='param6' /
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.23 /
&SPEC ID='CARBON MONOXIDE' /
&SPEC ID='WATER VAPOR' /
&SPEC ID='CARBON DIOXIDE' /
&SPEC ID='SOOT', TURBULENT_SCHMIDT_NUMBER=60. /

! Burner surface:
&SURF ID='Burner',  COLOR='RED', TMP_FRONT=200. /

# We have 1 in of Ceramic fiber board + 3mm of ceramic insulator layer: Numbers from NIST TN 2183 and
# https://www.thermaxxjackets.com/products/insulation-materials/ceramic/
&MATL ID='CeramicFiber', EMISSIVITY=1., DENSITY=300., SPECIFIC_HEAT=1.13, CONDUCTIVITY=0.153 /
&SURF ID='Compartment Wall', COLOR='SILVER', THICKNESS=0.028, MATL_ID='CeramicFiber', TMP_FRONT=150. /

# Compartment:
&OBST ID='Compartment floor', XB=-1.55, 0.03,-0.515, 0.515,-0.03,0.00, SURF_ID='Compartment Wall' /
&OBST ID='Compartment right', XB=-1.55, 0.03,-0.515,-0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment left',  XB=-1.55, 0.03, 0.495, 0.515, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment back',  XB=-1.55,-1.52,-0.495, 0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment front', XB= 0.00, 0.03,-0.495, 0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment roof',  XB=-1.55, 0.03,-0.515, 0.515, 0.99,1.02, SURF_ID='Compartment Wall' /

# Door entrance 43x80 cm:
&HOLE ID='Compartment door', XB=  0.01  , 0.04,-0.215, 0.215, 0.00,0.80, COLOR='GRAY'/

# Door opened:
&OBST ID='Open door',  XB= 0.0608, 0.4808,-0.245,-0.215, 0.00,0.80, SURF_ID='Compartment Wall'/

# Burner:
&MATL ID='Steel', CONDUCTIVITY=54, DENSITY=7800, SPECIFIC_HEAT=0.465, EMISSIVITY=0.8 /
&SURF ID='BurnerSteel', COLOR='GRAY 50', MATL_ID='Steel', THICKNESS=0.003, TMP_FRONT=150./

&OBST ID='Burner', XB=-1.46,-1.282,-0.089, 0.089, 0.088,0.24,
      SURF_ID6='BurnerSteel','BurnerSteel','BurnerSteel','BurnerSteel','BurnerSteel','Burner' /

# Burner Legs:
&OBST ID='BurnerLeg1', XB=-1.46 ,-1.44 ,-0.089,-0.069, 0.0,0.24, SURF_ID='BurnerSteel' /
&OBST ID='BurnerLeg2', XB=-1.46 ,-1.44 , 0.069, 0.089, 0.0,0.24, SURF_ID='BurnerSteel' /
&OBST ID='BurnerLeg3', XB=-1.302,-1.282,-0.089,-0.069, 0.0,0.24, SURF_ID='BurnerSteel' /
&OBST ID='BurnerLeg4', XB=-1.302,-1.282, 0.069, 0.089, 0.0,0.24, SURF_ID='BurnerSteel' /

# Devc for DT:
&DEVC ID='DT', XYZ=-1.10875,-0.25125,0.25375, QUANTITY='TIME STEP'/

# Devcs Lower Ignitor:
&DEVC ID='LI_X_F',   XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='LI_X_O2',  XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='LI_X_CO2', XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='LI_X_CO',  XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='LI_X_H2O', XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='LI_X_N2',  XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='LI_X_SOOT',XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VOLUME FRACTION', SPEC_ID='SOOT'/
&DEVC ID='LI_TMP',   XYZ=-1.10875,-0.25125,0.25375, QUANTITY='TEMPERATURE'/
&DEVC ID='LI_MIXT',  XYZ=-1.10875,-0.25125,0.25375, QUANTITY='MIXING TIME'/
&DEVC ID='LI_VEL',   XYZ=-1.10875,-0.25125,0.25375, QUANTITY='VELOCITY'/

# Devcs Mid Ignitor:
&DEVC ID='MI_X_F',   XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='MI_X_O2',  XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='MI_X_CO2', XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='MI_X_CO',  XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='MI_X_H2O', XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='MI_X_N2',  XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='MI_X_SOOT',XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VOLUME FRACTION', SPEC_ID='SOOT'/
&DEVC ID='MI_TMP',   XYZ=-1.10875,-0.00125,0.50625, QUANTITY='TEMPERATURE'/
&DEVC ID='MI_MIXT',  XYZ=-1.10875,-0.00125,0.50625, QUANTITY='MIXING TIME'/
&DEVC ID='MI_VEL',   XYZ=-1.10875,-0.00125,0.50625, QUANTITY='VELOCITY'/

# Slices:
&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0.015, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='VOLUME FRACTION', SPEC_ID='PROPANE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='MIXING TIME', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='EXTINCTION', CELL_CENTERED=T/


# Devcs results:
# Pressure tap:
&DEVC ID='Pres1', XYZ=-0.7,-0.47,0.495, QUANTITY='PRESSURE'/

# Total Fuel mass in the compartment:
&DEVC ID='FuelMass', XB=-1.52,0.0,-0.5,0.5,0.0,0.99, QUANTITY='DENSITY', SPEC_ID='PROPANE', SPATIAL_STATISTIC='VOLUME INTEGRAL' /

# Thermocouples, use Inconel 600, and sheath diameter:
# From https://asm.matweb.com/search/SpecificMaterial.asp?bassnum=NINC30:
&PROP ID='TC', DENSITY=8470, DIAMETER=0.0032, SPECIFIC_HEAT=0.444/
&DEVC ID='T0', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.79 /
